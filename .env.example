GEMINI_API_KEY=
GMAIL_CREDENTIALS_PATH=credentials.json
GMAIL_TOKEN_PATH=token.json

# Processing Settings
MAX_EMAILS_PER_SESSION=50
BATCH_SIZE=10
BATCH_DELAY_SECONDS=65
MAX_REQUESTS_PER_MINUTE=14

# Safety Settings
DRY_RUN=true
SAFE_ARCHIVE_MODE=true
CONFIDENCE_THRESHOLD=0.6
VERBOSE=false
ENABLE_CACHE=true
CACHE_PATH=.gmail_cleaner_cache.json
SESSION_SECRET=change-me
ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Gmail Query
GMAIL_QUERY=category:promotions newer_than:30d

# Custom Labels
KEEP_LABEL=AI_KEEP
ARCHIVE_LABEL=AI_ARCHIVED
REVIEW_LABEL=AI_REVIEW
